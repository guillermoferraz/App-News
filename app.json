{
  "expo": {
    "name": "news-app",
    "slug": "news-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logoAppNews.png",
    "splash": {
      "image": "./assets/img/logoAppNews.png",
      "resizeMode": "contain",
      "backgroundColor": "#B0C4DE"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#B0C4DE"
      },
      "package": "com.guillermoferraz.newsapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
